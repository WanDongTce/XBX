/* pages/songdetail/songdetail.wxss */
.reton{width: 18rpx;height: 32rpx;z-index: 999999;display: block;padding-top: 34rpx;margin: 0 auto}
.reton_sun{width: 100rpx;height: 100rpx;position: absolute;left: 4rpx;top: 30rpx;}
.header{position: fixed;left: 0;top: 0;line-height: 13vh;height: 10vh;text-align: left;display: block;width: 100%;background-color: #fff;}
.header text{display: inline-block;margin-left: 80rpx;line-height:80rpx;margin-top: -10rpx}
page{background-color: #fff}
.wrapBar{
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-bottom: 1rpx solid #ddd;
    padding:0 24rpx 20rpx;
}
.arrow-left{
    width: 44rpx;
    height: 44rpx;
}
.search{
    background-color: #eee;
    border-radius: 32rpx;
    height: 64rpx;
    margin: 0 auto;
    width: 510rpx;
    text-align: center
    
}
.yij{width: 88rpx;height: 88rpx;position: absolute;right: 0;top: 0;display: none}
.show{display: block}
.search text{font-size: 26rpx;color: #777777;display: inline-block;line-height: 64rpx;
background-image: url(https://m.ajihua888.com/wx_img/my/read2.png);background-size: 24rpx 24rpx;
padding-left: 40rpx;background-position: left center;background-repeat: no-repeat}
.search-input{
    padding: 10rpx 20rpx;
    font-size:30rpx;
    
}
.btn{
  width: 32rpx;height: 32rpx;
    padding-right: 20rpx;
    background-color: #fff;

}
/*list*/
.main{width: 686rpx;margin: 0 auto;margin-top: 40rpx}
.list{
    position: relative;
    flex-direction: row;
    justify-content: flex-start;
    float: left;
    width: 320rpx;
    border-radius: 6rpx;
  
   
    
}
.list:nth-child(2n){margin-left: 46rpx;margin-bottom: 48rpx}
.icon{
    position: absolute;
    top: 0;
    right: 0;
    width: 80rpx;
    height: 80rpx;
}
.image{
    width: 320rpx;
    height: 320rpx;
    /* border:1rpx solid red; */
    
    display: block
}

 .photo{ width: 320rpx;
    height: 320rpx;   background-image: url(data:image/png;base64,);border-radius: 10rpx;overflow: hidden}
.item{width: 100%;}
.item-title{
    font-size: 32rpx;
    color: #333;
    height: 32rpx;
    line-height: 32rpx;
    margin-bottom: 24rpx;
    padding-top: 32rpx;
    
}
.item-info{
    font-size: 28rpx;
    color: rgb(159, 159, 159);
    margin-bottom: 15rpx;
    text-overflow: ellipsis;
                overflow: hidden;
                white-space: nowrap;
}
.item-author{
    font-size: 28rpx;
    color: #999999;
}
/*popup*/
:host {
  font-size: 16px;
}

.center {
  width: 60%;
  padding: 20px;
  text-align: center;
  box-sizing: border-box;
}

.top {
  color: #fff;
  width: 100%;
  padding: 20px;
  border-radius: 0;
  line-height: 20px;
  background-color: rgba(0, 0, 0, 0.8)!important;
}

.bottom {
  width: 100%;
  padding: 20px;
  line-height: 100px;
  background-color: #fff;
}

.right {
  width: 75%;
  height: 100%;
  padding: 20px;
}
.list_sun{width: auto !important;line-height: 54rpx;padding-left: 26rpx;padding-right: 26rpx;background-color: #F4F4F4;
border-radius: 27rpx;font-size: 26rpx;margin-right: 26rpx;margin-bottom: 52rpx;float: left}
van-popup .name{margin-bottom: 30rpx;display: block}
.active{background-color: #48B947}
.clearfix{clear: both}
.van-popup{position: relative}
.chongzhi{position: absolute;left: 62rpx;bottom:34rpx;width: 180rpx;height: 70rpx;display: block}
.chongzhi button{display: block;width: 100%;height: 100%;padding: 0;line-height: 70rpx;border-radius: 35rpx;overflow: hidden}
.quding{position: absolute;right: 62rpx;bottom:34rpx;width: 180rpx;height: 70rpx;display: block;
}
.quding button{display: block;width: 100%;height: 100%;padding: 0;line-height: 70rpx;border-radius: 35rpx;overflow: hidden;
background-color: #48B949;color: #fff}
.fixed{
  position: fixed;
  bottom: 0;
  right: 0;
  width: 75%;
  z-index: 9999;
}