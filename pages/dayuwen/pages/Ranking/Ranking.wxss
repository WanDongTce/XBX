.clearfix{clear: both}
page{background-color: #fff}
.box{width: 654rpx;margin: 0 auto;border: 1px solid #E6E6E6;height: 262rpx;border-radius: 10rpx;
margin-top: 30rpx}
.reton{width: 18rpx;height: 32rpx;z-index: 999999;display: block;padding-top: 34rpx;margin: 0 auto}
.reton_sun{width: 100rpx;height: 100rpx;position: absolute;left: 4rpx;top: 30rpx;}
.header{position: fixed;left: 0;top: 0;line-height: 13vh;height: 10vh;text-align: center;display: block;width: 100%;background-color: #fff;}
.information{background-color: #fff;padding-top: 30rpx;border-radius: 10rpx 10rpx 0 0 }
.information .content_box{width: 690rpx;margin: 0 auto;}
.information .content_box .photo{width: 100rpx;height: 100rpx;background-color: #eee;float: left;
border-radius: 10rpx;overflow: hidden;margin-left: -32rpx}
.information .content_box .photo image{width: 100%;height: 100%}
.information .content_box .information_sun{float: left;width: 526rpx;margin-left: 28rpx}
.information .content_box .information_sun .name text{font-size: 32rpx;display: block;height: 32rpx;line-height: 32rpx}
.information .content_box .information_sun .author text{font-size: 24rpx;display: inline-block;height: 24rpx;line-height: 24rpx;
color: #999999;margin-right: 20rpx}
.information .content_box .information_sun .author{margin-top: 30rpx}
.information .content_box .information_sun .appreciation text{font-size: 26rpx;display: block;height: 24rpx;line-height: 24rpx;
color: #333333}
.information .content_box .information_sun .appreciation{margin-top: 30rpx}
.information .content_box .information_sun{position: relative}
.information .content_box .information_sun .hear{position: absolute;right: 0;top: 43rpx;width: 130rpx;height: 50rpx}
.information .content_box .information_sun .hear text{display: block;line-height: 50rpx;text-align: center;
border: 2rpx solid #48B947;color: #48B947;border-radius: 50rpx;font-size: 26rpx}
.function{margin: 0 auto;width: 590rpx;border-top: 1px solid #E6E6E6;padding-top: 24rpx;padding-bottom: 24rpx;
margin-top: 30rpx;}
.function .appreciation{float: left}
.function .appreciation text{font-size: 26rpx;color: #333}
.function .hear{float: right;background-color: #F4F4F4;border-radius: 24rpx}
.function .hear text{font-size: 26rpx;color: #333;display: block;line-height: 48rpx;padding-left: 20rpx;padding-right: 20rpx;}
.clearfix{clear: both}




.btn{margin-top: 30rpx;border-bottom: 2rpx solid #E8E8E8}
.btn .list{width: 50%;float: left;background-color: #fff;text-align: center;}
.btn .list text{display: inline-block;font-size: 28rpx;height: 28rpx;padding-top: 30rpx;padding-bottom: 28rpx;}
.btn .active text{border-bottom: 2rpx solid #4BACE2;color: #4BACE2}
.list_box{background-color: #fff;margin-bottom: 20rpx}
.list_box .content_box{width: 690rpx;margin: 0 auto;}
.list_box .list{height: 120rpx;border-bottom: 2rpx #e8e8e8 solid}
.list_box .list .on{float: left;width: 65rpx}
.list_box .list .on text{font-size: 30rpx;line-height: 120rpx}
.list_box .list .on image{width: 48rpx;height: 40rpx;display: none;padding-top: 35rpx}
.list_box .list:nth-child(1) text{ display: none}
.list_box .list:nth-child(1) image{display: block}
.list_box .list:nth-child(2) text{ display: none}
.list_box .list:nth-child(2) image{display: block}
.list_box .list:nth-child(3) text{ display: none}
.list_box .list:nth-child(3) image{display: block}
.list_box .list .photo{width: 60rpx;height: 60rpx;border-radius: 50%;float: left;margin-top: 30rpx;overflow: hidden;background-image: url(data:image/png;base64,);background-size: 100% 100%}
.list_box .list .photo image{width: 100%;height: 100%}
.list_box .list .name{float: left;margin-left: 13rpx}
.list_box .list .name text{font-size: 32rpx;display: block;height: 
32rpx;line-height: 120rpx}
.list_box .list .number{float: right}
.list_box .list .number image{width: 38rpx;height: 34rpx;float: left;padding-top: 43rpx;margin-right: 15rpx}
.list_box .list .number text{font-size: 32rpx;display: block;height: 32rpx;line-height: 120rpx;float: left}
.show{display: block}
.hidden{display: none}
.list_box .list .time{float: left}
.list_box .list .time text{font-size: 32rpx;display: block;height: 32rpx;line-height: 120rpx}
.Recitation{background-color: #fff;position: fixed;left: 0;bottom: 0;width: 100%;padding-bottom: 56rpx;padding-top: 40rpx}
.Recitation .btn_sun{width: 260rpx;height: 70rpx;line-height: 70rpx;margin: 0 auto;background-color: #4BACE2;
 border-radius: 35rpx}
.Recitation .btn_sun text{display: block;line-height: 70rpx;text-align: center;color: #fff;font-size: 32rpx;}