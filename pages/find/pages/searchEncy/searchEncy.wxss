page {
    background-color: #f9f9f9;
}

.searchbigbox {
    position: fixed;
    top: 10vh;
    width: 100%;
    height: 10vh;
    background-color: #fff;
    justify-content: space-between;
    align-items: center;
}

.searchbox {
    height: 7vh;
    margin: 0 24rpx;
    border-radius: 10rpx;
    background-color: #f1f1f1;
    align-items: center;
    padding: 0 20rpx;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}

.search_img {
    width: 26rpx;
    height: 24rpx;
}


.searchbox input {
    font-size: 28rpx;
    color: #333;
    margin-left: 22rpx;
    width: 500rpx;
    height: 100%;
    line-height: 7vh;
    margin-right: 22rpx;
}

.search_btn {
    font-size: 32rpx;
    color: #4bace2;
    margin-right: 24rpx;
}
.box{
    width: 100%;
    margin-top: 10vh;
}
.cont{
    width: 96%;
    margin: 16rpx auto 30rpx;
    background: #fff;
    border-radius: 10rpx;
}
.tit{
    width: 100%;
    padding: 40rpx 20rpx;
    border-bottom: 1rpx solid #dfdfdf;
    font-size: 28rpx;
    color: #999;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}
.list{
    width: 100%;
    padding: 0 20rpx;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}
.item{
    width: 100%;
    border-bottom: 1rpx solid #dfdfdf;
    font-size: 32rpx;
    color: #333;
    padding: 40rpx 0;
}
.item:last-child{
    border-bottom: none;
}
.item2{
    display: flex;
    display: -webkit-flex;
    align-items: center;
}
.img{
    width: 80rpx;
    height: 80rpx;
    margin-right: 20rpx;
}
.text{
    width: 80%;
    font-size: 32rpx;
    color: #333;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}