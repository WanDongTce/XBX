.list{
    width: 100%;
    background: #fff;
}
.item{
    width: 100%;
    padding: 24rpx;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    border-bottom: 1rpx solid #dfdfdf;
    display: flex;
    display: -webkit-flex;
    align-items: center;
    justify-content: space-between;
}
.item:last-child{
    border-bottom: none;
}
.lt{
    width: 90%;
    font-size: 30rpx;
    color: #333;
}
.icon{
    width: 35rpx;
    height: 35rpx;
}