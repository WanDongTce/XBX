page{
  /* background-color:#bbb; */
  background-color:#f4f4f4;
  color:#333;
}
.component-class {
    border-bottom: 1px solid #dfdfdf;
}
.item_box{
  padding:10rpx 24rpx 40rpx;
  background-color:#fff;
  margin-bottom:16rpx;
}
.time{
  font-size:30rpx;
  line-height:74rpx;
  border-bottom:1rpx solid #dfdfdf;
}
.address{
  font-size:28rpx;
  line-height:40rpx;
  margin-top:12rpx;
  margin-bottom:14rpx;
  word-break: break-all;
}
.item_center{
  background-color:#f8f8f8;
   padding:32rpx; 
   
}
.img{
  width:172rpx;
  height:172rpx; 
  /* margin-left:32rpx;  */
  border-radius:10rpx;
}
.ml{
  margin-left:28rpx;
  width:455rpx;
}
.title{
  font-size:26rpx;
  word-break: break-all;
  line-height:40rpx;
  min-height: 80rpx;
}
.item_bottom{
  justify-content: space-between;
  margin-top:45rpx;
  align-items: center;
}
.number{
  font-size:24rpx;
  color:#666;
}
.integral{
  font-size:24rpx;
  color:#666;
}
.integral text{
  color:#4bace2;
  font-size:36rpx;
  margin-right:10rpx;
}
.component-class{
    background-color:#f4f4f4 !important;
