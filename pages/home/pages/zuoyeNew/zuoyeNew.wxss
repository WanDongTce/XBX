}

.sys-msg-box {
    width: 95%;
    padding: 2% 0;
    margin: 30rpx auto;
    text-align: center;
}

.sys-msg {
    padding: 2% 10rpx;
    font-size: 26rpx;
    color: #fff;
    background: #d4d4d4;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    border-radius: 10rpx;
    word-break: break-all;
    line-height: 1.2em;
}

.bottom-box {
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 8vh;
    display: flex;
    display: -webkit-flex;
    align-items: center;
    justify-content: space-around;
    padding: 1vh 0;
    background: #f4f4f4;
}
.bottom-box .input-cont{
    width: 95%;
    height: 6vh;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    display: flex;
    display: -webkit-flex;
    align-items: center;
    justify-content: space-between;
    overflow: hidden;
}
.bottom-box .input-box {
    width: 90%;
    height: 100%;
    font-size: 26rpx;
    color: #333;
    padding: 0 2%;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    border-radius: 10rpx;
    border: 1rpx solid #dfdfdf;
    background: #fff;
}
.btnnew{
    width: 10%;
    height: 6vh;
    color: #888;
    font-size: 28rpx;
    text-align: right;
    line-height: 6vh;
}

.bottom-box .input-box input {
    width: 100%;
    height: 100%;
}
.place{
    color: #999;
}
/* 缴费 */
.renewal-wrap{
  margin: 30rpx 32rpx;
  height: auto;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}
.renewal.active{
  border: 1rpx solid #4BACE3;
}
.renewal {
  display: flex;
  flex: 0 0 auto;
  justify-content: space-between;
  align-items: center;
  background-color: white;
  padding: 20rpx;
  border-radius: 4rpx;
  border: 1rpx solid #eee;
  margin: 1rpx;
}
.footer{
  position: fixed;
  bottom: 0;
  width: 100%;
  background-color: white;
  height: 100rpx;
  border: 1rpx solid #ddd;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 10rpx 0;
}
.footer-btn{
  background-color: #4BACE3;
  color: white;
  border-radius: 40rpx;
  width: 80%;
}
.footer-btn::after{
  outline: none;
}
.banner{
  margin-bottom: 25rpx;
  width: 100%;
}
.bg-info{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
}
.bg-info-text{
  margin-bottom: 18rpx;
  font-size: 28rpx;
  color: #666;
}