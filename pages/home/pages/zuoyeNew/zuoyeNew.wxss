/*导航栏  */
.navigation-box {
    position: fixed;
    top: 0;
    left: 0;
    z-index: 100;
    background-color: #4bace2;
    width: 100%;
    height: 10vh;
    padding: 0 30rpx;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}
.back-box{
    position: absolute;
    left: 30rpx;
    top: 0;
    width: 100rpx;
    height: 10vh;
}
.navigation-back-icon {
    position: absolute;
    left: 0;
    top: 62%;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
    width: 18rpx;
    height: 32rpx;
}
.navigation-tit{
    width: 50%;
    height: 100%;
    line-height: 13vh;
    text-align: center;
    color: #fff;
    font-size: 33rpx;
    margin: 0 auto;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
