/* 聊天 */
.boxnew {
    /* position: fixed; */
    /* bottom: 0; */
    /* left: 0; */
    width: 100%;
    /* height: 90vh; */
    padding: 0 30rpx 10vh;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    /* overflow-y: scroll; */
    background-color: transparent;
}

.itemnew {
    width: 100%;
    padding: 4% 0;
    display: flex;
    display: -webkit-flex;
    align-items: flex-start;
}

.self-box {
    justify-content: flex-end;
}

.head-img {
    width: 10vw;
    height: 10vw;
}
.rt-box{
    width: 90%;
}
.user{
    width: 100%;
    color: #999;
    font-size: 26rpx;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    margin-bottom: 10rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.user-1{
    text-align: right;
    padding-right: 5%;
}
.user-2{
    text-align: left;
    padding-left: 5%;
}
.cont-box {
    max-width: 95%;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    display: flex;
    display: -webkit-flex;
    align-items: flex-start;
    
}
.cont-box-1{
    margin-left: 2.5%;
    justify-content: flex-end;
}
.cont-box-2{
    margin-right: 2.5%;
    justify-content: flex-start;
}

.msg {
    /* padding: 20rpx 30rpx 20rpx 20rpx; */
    padding: 20rpx;
    background: #4bace2;
    
    font-size: 30rpx;
    color: #fff;
    white-space: normal;
    word-break: break-all;
    line-height: 1.5em;
    background-size: 100% 100%;
    border-radius: 10rpx;
}

.msg2 {
    /* padding: 20rpx 20rpx 20rpx 30rpx; */
    background: #fff;
    color: #333;
}
