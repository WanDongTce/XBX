.container1{
    height: 7vh;
    width: 100%;
    background-color: beige; 
    display: flex;
    justify-content: space-between;
    padding: 10rpx 1%;
    box-sizing: border-box;
}
.item1{
    height: 5vh;
    width: 8%;
    border: 1rpx solid #fff;
    line-height: 5vh;
    text-align: center;
    font-size: 30rpx;
    color: transparent;
}
.item1.selected-color{    
    color: #fff;
}
.item1.selected-color.color7{
    color: #222;
}

.item1font{
  margin-left: 10rpx;
  margin-right: 10rpx;
  height:30px;
  line-height: 30px;
  font-size: 14px;
}

.item2font{
  margin-top: 3px;
  width: auto;
  height:25px;
  line-height: 25px;
  font-size: 14px;
  color: white;
  background-color: #0088fe;
  padding-left: 5rpx;
  padding-right: 5rpx;
  border-radius: 5px;
  display: block;
}
.container2{
    width: 100%;
    height: 70vh;
    background-position: 0 0;
    background-repeat: no-repeat;
    background-size: 100% 100%;
}
canvas{
    width: 100%;
    height: 100%;
}

.container3{
  height: 13vh;
  width: 100%;
  display: flex;
  justify-content: space-around;
  padding: 1% 2%;
  box-sizing: border-box;
}


.item2{
    width: 35%;
    height: 11vh;
    text-align: center;
    line-height: 11vh;
    border-radius: 10rpx;
    border: 1rpx solid #0088fe;
    font-size: 30rpx;
    color: #0088fe;
}
.item3{
  width: 11vh;
  height: 11vh;
  font-size: 28rpx;
  text-align: center;
  border-radius: 50%;
  background-color: red;
  color: #fff;
}
.item3 view{
    width: 100%;
    height: 3.5vh;
    line-height: 3.5vh;
    display: inline-block;
    margin-top: 2vh;
}
.item3 .view1{
    height: 5vh;
    line-height: 5vh;
    margin-top: 3vh;
}
.item3 .endluzhi{
