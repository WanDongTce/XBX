.item-img {
    width: 150rpx;
    height: 150rpx;  
    border-radius: 10rpx;
    margin-left: 24rpx;
}

.item-info {
    width: 60%;
    margin-left: 24rpx;
}

.item-name {
    width: 100%;
    height: 80rpx;
    font-size: 32rpx;
    color: #333;
    word-break: break-all;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    line-height: 40rpx;
    margin-bottom: 40rpx;
    white-space: normal;
}
.item-tips{
    width: 100%;
    height: 50rpx;
    display: flex;
    -webkit-display: flex;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}
.item-price{
    width: 40%;
    height: 100%;
    font-size: 30rpx;
    color: #F01B1B;
}
.item-count-box{
    width: 50%;
    height: 100%;
    text-align: right;
    display: flex;
    display: -webkit-flex;
    align-items: center;
    justify-content: flex-end;
    /* border: 1rpx solid #dfdfdf; */
    border-radius: 10rpx;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}
.item-count-box > view{
    width: 33.33%;
    height: 100%;
    text-align: center;
}
.minus,.plus{
    font-weight: bold;
    font-size: 30rpx;
    color: #333;
    line-height: 50rpx;
    background: #fff;
}
.disabled{
    color: #ddd;
    /* background: #f4f4f4; */
    pointer-events:none
}
.num{
    line-height: 50rpx;
