/*收费提示*/
.center{
  width: 80%;
  text-align: center;
  box-sizing: border-box;
  border-radius: 12rpx;
}
.popwrap{
display: flex;
flex-direction: column;
justify-content: center;
align-items: center;
padding-bottom: 40rpx;
position: relative;
}
.popwrap-img{
  margin-bottom: 30rpx;
  overflow: hidden;
}
.text{
  font-size: 32rpx;
  font-weight: 400;
  color: #333;
}
.text .num{
  color:#16b7ff;
  font-size: 80rpx;
  font-weight: 700;
  margin-left: 5rpx;
  margin-right: 5rpx;
}
.popwrap-close{
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  height: 32rpx;
  width: 32rpx;
}
.pop-text{
  font-size: 28rpx;
  margin-bottom: 40rpx;
  color: #666;
}
.pop-btn{
margin-bottom: 30rpx;
border-width: 0;
width: 60%;
background-color: #4BACE2;
color: white;
border-radius: 46rpx;
}
.pop-btn-outline{
  background-color: white;
  color: #4BACE2;
  border: 1px solid #4BACE2;
}
button.pop-btn::after{
  content: "";
}
.isvip{
  overflow: hidden;
}

