.school-box {
    width: 100%;
    background: #fff;
    margin-bottom: 18rpx;
    overflow: hidden;
}

.h2 {
    width: 100%;
    padding: 30rpx 24rpx;
    display: flex;
    display: -webkit-flex;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
}

.h2-lt {
    display: flex;
    display: -webkit-flex;
    align-items: center;
}

.rect-icon {
    width: 8rpx;
    height: 28rpx;
    background: #48b947;
    margin-right: 20rpx;
}

.h2-text {
    color: #333;
    font-size: 28rpx;
}

.h2-rt {
    width: 20%;
    display: flex;
    display: -webkit-flex;
    align-items: center;
    justify-content: flex-end;
}

.h2-rt-text {
    color: #48b947;
    font-size: 24rpx;
    margin-right: 10rpx;
}

.h2-rt-img {
    width: 20rpx;
    height: 20rpx;
}

.news {
    width: 90%;
    padding: 30rpx 0 25rpx;
    border-bottom: 1rpx solid #dfdfdf;
    display: flex;
    display: -webkit-flex;
    align-items: center;
    justify-content: space-between;
    margin: 0 auto;
}

.news:last-child {
    border-bottom: none;
}

.news-lt {
    width: 242rpx;
    height: 148rpx;
    margin-right: 37rpx;
    overflow: hidden;
}

.news-img {
    width: 100%;
    height: 100%;
    border-radius: 20rpx;
}

.news-rt {
    width: 60%;
}

.news-tit {
    width: 100%;
    height: 80rpx;
    font-size: 28rpx;
    color: #333;
    line-height: 40rpx;
    overflow: hidden;
    word-break: break-all;
    margin-bottom: 15rpx;
}

.news-tips {
    width: 100%;
    display: flex;
    display: -webkit-flex;
    align-items: center;
    justify-content: space-between;
}

.tips-lt {
    width: 40%;
    font-size: 24rpx;
    color: #666;
}

.tips-rt {
    width: 60%;
    text-align: right;
}

.news-tip-img {
    width: 26rpx;
    height: 24rpx;
    display: inline-block;
    vertical-align: middle;
    margin-right: 8rpx;
}

.news-tip-text {
    color: #666;
    font-size: 24rpx;
    margin-right: 34rpx;
    display: inline-block;
    vertical-align: middle;
}

.news-tip-text:last-child {
    margin-right: 0;
}


