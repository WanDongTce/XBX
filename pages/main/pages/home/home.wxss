/* 续费弹窗 */
.renewbox{
    position:fixed;
    left:0;
    top:0;
    z-index:200;
    width:100%;
    height:100vh;
    background:rgba(0,0,0,.8);
}
.renew_smbox{
     position:relative;
    top:50%;
    left:50%;
    transform:translate(-50%, -50%);
    -webkit-transform:translate(-50%, -50%);
    width:73%;    
    background-color:#fff;
    border-radius: 10rpx;
}
.renew_title{
    font-size:36rpx;
    color:#333;
    font-weight:bold;
    text-align: center;
    background-color:#fff;
    border-bottom:2rpx solid #E5E5E5;
    line-height:112rpx;
    height:112rpx;
    box-sizing: border-box;
    border-top-left-radius: 10rpx;
    border-top-right-radius: 10rpx;
}
.renew_content{
    font-size:30rpx;
    color:#333;
    text-align: center;
    margin:92rpx 30rpx 104rpx;
    line-height:54rpx;
}
.renew_bottom{
    color:#48b947;
    height:98rpx;
    line-height:98rpx;
    font-size:32rpx;
    text-align: center;
    border-top:1rpx solid #e4e4e4;
}
.activity_imgbox{
    width:100%;   
}
.activity_imgbox image{
    width:100%;
}
.activity_imgbox_change{
    width:100%; 
    position:fixed;
    top:0;  
    z-index: 1000;
}
.activity_imgbox_change image{
    width:100%;
}