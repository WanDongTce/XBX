.header{position: fixed;left: 0;top: 0}
.clearfix{ clear: both}
page{background-color: #fff}
.reton{width: 40rpx;height: 42rpx;z-index: 999999;display: block;padding-top: 34rpx;margin: 0 auto;}
.reton_sun{width: 100rpx;height: 100rpx;position: absolute;left: 4rpx;top: 30rpx;}
.reton_sun .inf{width: 12rpx;height: 12rpx;background-color: #ff6363;border-radius: 50%;position: absolute;top: 35rpx;right: 30rpx;display: none}
.reton_sun .bl{display: block}
.header{position: fixed;left: 0;top: 0;line-height: 13vh;height: 10vh;text-align: center;display: block;width: 100%;background-color: #fff;border: 1px solid #dfdfdf}
.list_box{margin-top: 30rpx}
.list_box .list{width: 686rpx;margin: 0 auto;box-shadow:0 0 20rpx 3rpx rgba(0, 0, 0, 0.2);margin-bottom: 40rpx;padding-top: 32rpx;
padding-bottom: 32rpx}
.list_box .list .top .pic{width: 88rpx;height: 88rpx;border-radius: 50%;float: left;overflow: hidden}
.list_box .list .top .pic image{width: 100%;height: 100%}
.list_box .list .top .name_box{width: 501rpx;float: right}
.list_box .list .top .name_box .name text{font-size: 32rpx;height: 32rpx;line-height: 32rpx}
.list_box .list .top{width: 621rpx; margin: 0 auto}
.list_box  .list .top  .name_box .Star{margin-top: 18rpx}
.list_box  .list .top  .name_box .Star .Star_sun{width: 26rpx;height: 24rpx;background-image: url(data:image/png;base64,);
background-size: 26rpx 24rpx;float: left;margin-right: 5rpx}

.list_box{margin-bottom: 140rpx}

.list_box  .list .top  .name_box .Star .Star_sun image{width: 26rpx;height: 24rpx;display: block}
.list_box  .list .top  .name_box .Star .Star_sun image{display: none}
.list_box  .list .top  .name_box .Star .Star_sun .active{display: block}
.list_box  .list .top  .name_box .Star .Fraction{float: left;font-size: 23rpx;display: block;line-height: 23rpx;height: 23rpx;margin-left: 7rpx}
.list_box  .list .top .name_box .address{ float: right;height: 24rpx;line-height: 24rpx;font-size: 22rpx;
text-align: right;color: #999999;background-repeat: no-repeat;background-size: 21rpx 24rpx;padding-left: 27rpx}
.list_box .list .bottom_box{width: 621rpx; margin: 0 auto;margin-top: 32rpx}
.list_box .list .bottom_box .photo01{width: 192rpx;height: 192rpx;float: left;border-radius: 10rpx;overflow: hidden}
.list_box .list .bottom_box .photo01 image{width: 100%;height: 100%;}
.list_box .list .bottom_box .photo01:nth-child(1){margin-right: 23rpx}
.list_box .list .bottom_box .photo01:nth-child(2){margin-right: 23rpx}
.footer .list{width: 20%;float: left}
.footer{height: 100rpx;width: 100%;position: fixed;left: 0;bottom: 0;background-color: #fff}
.footer .list:nth-child(1) image{width: 50rpx;height: 44rpx;display: block;margin: 0 auto;padding-top: 15rpx}
.footer .list:nth-child(2) image{width: 49rpx;height: 47rpx;display: block;margin: 0 auto;padding-top: 13rpx}
.footer .list:nth-child(3) image{width: 49rpx;height: 46rpx;display: block;margin: 0 auto;padding-top: 14rpx}
.footer .list:nth-child(4) image{width: 51rpx;height: 47rpx;display: block;margin: 0 auto;padding-top: 14rpx}
.footer .list:nth-child(5) image{width: 41rpx;height: 47rpx;display: block;margin: 0 auto;padding-top: 13rpx}
.footer .list{text-align: center}
.footer .list text{font-size: 18rpx;display: block;line-height: 18rpx;height: 18rpx;margin-top: 10rpx}
.none_box image{width: 201rpx;height: 131rpx;display: block;margin: 0 auto;padding-top: 151rpx}
.none_box{text-align: center}
.none_box text{font-size: 28rpx;height: 28rpx;line-height: 28rpx;margin-top: 28rpx;display: block;color: #aeaeae}