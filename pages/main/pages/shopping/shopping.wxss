.clearfix{ clear: both}
page{background-color: #fff}
.header{position: fixed;left: 0;top: 0;line-height: 13vh;height: 10vh;text-align: center;display: block;width: 100%;background-color: #fff;border: 1px solid #dfdfdf}
.list_box{margin-top: 30rpx;}
.list_box .content_box{padding-left: 30rpx;padding-right: 30rpx}
.list_box .content_box .list{ height: 468rpx;box-shadow:0 0 20rpx 3rpx rgba(0, 0, 0, 0.2);border-radius: 8rpx;margin-bottom: 30rpx}
.list_box .content_box .list .top {padding-left: 30rpx;padding-right: 30prx;padding-top: 30rpx}
.list_box .content_box .list .top .left .photo{width: 100rpx;height: 100rpx;border-radius: 50%;overflow: hidden}
.list_box .content_box .list .top .left .photo .box{background-color: red;width: 100%;height: 100%}
.list_box .content_box .list .top .left .photo{float: left}
.list_box .content_box .list .top .left .name_box{float: left;margin-left: 18rpx;width: 78%;}
.list_box .content_box .list .top .left .name_box .name{padding-top: 10rpx}
.list_box .content_box .list .top .left .name_box .name text{font-size: 30rpx;height: 30rpx;line-height: 30rpx;display: block}
.list_box .content_box .list .top .left .name_box .Star{margin-top: 18rpx}
.list_box .content_box .list .top .left .name_box .Star .Star_sun{width: 26rpx;height: 24rpx;background-image: url(data:image/png;base64,);
background-size: 26rpx 24rpx;float: left;margin-right: 5rpx}
.list_box .content_box .list .top .left .name_box .Star .Star_sun image{width: 26rpx;height: 24rpx;display: block}
.list_box .content_box .list .top .left .name_box .Star .Fraction{float: left;font-size: 23rpx;display: block;line-height: 23rpx;height: 23rpx;margin-left: 7rpx}
.list_box .content_box .list .top .left .name_box .address{ float: right;height: 24rpx;line-height: 24rpx;font-size: 20rpx;
text-align: right;color: #48b947;background-image: url(data:image/png;base64,);background-repeat: no-repeat;background-size: 21rpx 24rpx;padding-left: 27rpx}

.list_box .content_box .list .bottom_box{padding-left: 30rpx;padding-right: 30rpx;margin-top: 26rpx}
.list_box .content_box .list .bottom_box .photo01{width: 394rpx;height: 276rpx;background-color: red;border-radius: 10rpx;
float: left}
.list_box .content_box .list .bottom_box .photo02{width: 220rpx;height: 130rpx;background-color: red;border-radius: 10rpx;
float: right}
.list_box .content_box .list .bottom_box .photo03{width: 220rpx;height: 130rpx;background-color: red;border-radius: 10rpx;
float: right;margin-top: 18rpx}

.none_box image{width: 201rpx;height: 131rpx;display: block;margin: 0 auto;padding-top: 151rpx}
.none_box{text-align: center}
.none_box text{font-size: 28rpx;height: 28rpx;line-height: 28rpx;margin-top: 28rpx;display: block;color: #aeaeae}
.footer .list{float: left;width: 20%}
.footer .list:nth-child(1) image{width: 49rpx;height: 42rpx;padding-top: 5rpx}
.footer .list:nth-child(2) image{width: 44rpx;height: 44rpx;padding-top: 3rpx}
.footer .list:nth-child(3) image{width: 50rpx;height: 47rpx}
.footer .list:nth-child(4) image{width: 42rpx;height: 42rpx;padding-top: 5rpx}
.footer .list:nth-child(5) image{width: 37rpx;height: 42rpx;padding-top: 5rpx}
.footer{ position: fixed;left: 0;bottom: 0;width: 100%;padding-top: 15rpx;padding-bottom: 15rpx}
.footer .list image{display: block;margin: 0 auto}
.footer .list text{font-size: 20rpx;display: block;text-align: center;margin-top: 6rpx}