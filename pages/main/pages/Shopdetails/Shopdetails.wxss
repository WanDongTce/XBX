.header{position: fixed;left: 0;top: 0}
.clearfix{ clear: both}
page{background-color: #fff}
.reton{width: 18rpx;height: 32rpx;z-index: 999999;display: block;padding-top: 34rpx;margin: 0 auto}
.reton_sun{width: 100rpx;height: 100rpx;position: absolute;left: 4rpx;top: 30rpx;}
.header{position: fixed;left: 0;top: 0;line-height: 13vh;height: 10vh;text-align: center;display: block;width: 100%;background-color: #fff;border: 1px solid #dfdfdf}
.banner swiper{width: 100%;height: 350rpx}
.banner swiper swiper-item{width: 100%;height: 100%}
.banner swiper swiper-item image{width: 100%;height: 100%}
.information{background-color: #fff;width: 750rpx;height: 160rpx;}
.information .content_box{width: 690rpx;margin: 0 auto;padding-top: 30rpx}
.information .content_box .photo{width: 100rpx;height: 100rpx;background-color: red;border-radius: 50%;float: left;overflow: hidden}
.information .content_box .photo image{width: 100%;height: 100%;}
.information .content_box .name_box{float: left;margin-left: 24rpx;width: 566rpx;padding-top: 13rpx;}
.information .content_box .name_box .top .name{ float: left}
.information .content_box .name_box .top .name text{font-size: 32rpx;display: block;height: 32rpx;line-height: 32rpx;}
.information .content_box .name_box .bottom text{font-size: 24rpx;height: 24rpx;line-height: 24rpx;display: block}
.information .content_box .name_box .bottom{margin-top: 19rpx}
.information .content_box .name_box .Star{float: right}
.information .content_box .name_box .Star .Star_sun{width: 26rpx;height: 24rpx;background-image: url(data:image/png;base64,);
background-size: 26rpx 24rpx;float: left;margin-right: 5rpx}
.information .content_box .name_box .Star .Star_sun image{width: 26rpx;height: 24rpx;display: block}
.information .content_box .name_box .Star .Fraction{float: left;font-size: 23rpx;display: block;line-height: 23rpx;height: 23rpx;margin-left: 7rpx}
.list_box .btn{width: 200rpx;background-color: #FAFAFA;float: left;height: 100%}
.list_box .btn .btn_sun{text-align: center}
.list_box .btn .active{background-color: #fff}
.list_box .btn .btn_sun text{font-size: 30rpx;color: #333333;display: inline-block;line-height: 96rpx;}
.list_box .list_sun{float: left;width: 550rpx;height: 100%;overflow-y:scroll}
.list_box .list_sun .list{margin-bottom: 48rpx}
.list_box .list_sun .photo{width: 120rpx;height: 120rpx;float: left;margin-left: 32rpx}
.list_box .list_sun .right{float: right;width: 316rpx;margin-right: 32rpx}
.list_box .list_sun .photo image{display: block;width: 100%;height: 100%}
.list_box .list_sun .right .name{width: 100%;text-overflow: ellipsis;
                overflow: hidden;
                white-space: nowrap;}
.list_box .list_sun .right .name text{font-size: 30rpx;color: #333333;}
.list_box  .list .money_box{width: 290rpx;margin: 0 auto;margin-top: 11rpx}
.list_box  .list .money_box .money{ float: left}
.list_box  .list .money_box .money text{font-size: 24rpx;display: block;line-height: 36rpx;height: 36rpx}
.list_box  .list .money_box .number{float: right}
.list_box  .list .money_box .number .disabled{display: none}
.list_box  .list .money_box .number .normal{display: block}
.list_box  .list .money_box .number image{float: left;display: block;width: 36rpx;height: 36rpx;}
.list_box  .list .money_box .number text{display: block;float: left;font-size: 22rpx;height: 36rpx;line-height: 36rpx;
padding-left: 15rpx;padding-right: 15rpx}