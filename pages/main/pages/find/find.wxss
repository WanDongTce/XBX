/* pages/find/find.wxss */
.newbox{
  padding:0 30rpx;
  background-color:#fff;
}
.head{
  /* width: 92%; */
  height: 140rpx;
  /* margin-left: 30rpx; */
  display: flex;
  display: -webkit-flex;
  align-items: center;
  border-bottom: solid 1rpx #f2f1f1;
  background-color: #fff;
}
.hide{display: none}
.one{
  margin-top: 10rpx;
  border-radius: 10rpx 10rpx 0rpx 0rpx;
}
.two{
  border-radius: 0rpx;
}
.three{
  border-radius: 0rpx 0rpx 10rpx 10rpx;
}
.head-img{
  width: 88rpx;
  height: 88rpx;
  /* margin-left: 30rpx; */
}
.head-box{
  width: 500rpx;
  height: 88rpx;
  margin-left: 30rpx;
  font-size: 0;
}
.head-box-a{
  width: 500rpx;
  height: 51rpx;
  margin-top:9rpx;
}
.head-text-a{
  font-size: 30rpx;
  color: #000;
}
.head-text-b{
  font-size: 24rpx;
  color: #a3a3a3;
}
.hide_sun{display:none !important;}
.layer_box{position: fixed;width: 100%; height: 100%;background-color: rgba(0, 0, 0, 0.7);left: 0;top: 0;
display: flex;align-items: center;justify-content: center;}
.layer_box .layer{width: 624rpx;height: 728rpx;background-image:url(http://m.54xuebaxue.com/wx_img/img/login/bd01.png);background-size: 100% 100%}
.layer_box .layer .name{font-size: 38rpx; color: #333333;padding-top: 382rpx;display: block;text-align: center;
height: 38rpx;line-height: 38rpx}
.layer_box .layer .text{font-size: 30rpx;color: #999999;display: block;text-align: center}
.layer_box .layer .btn{width: 532rpx;height: 82rpx;   background-image: -moz-linear-gradient( 0deg, rgb(255,184,99) 0%, rgb(255,189,62) 50%, rgb(243,165,14) 100%);
  background-image: -webkit-linear-gradient( 0deg, rgb(255,184,99) 0%, rgb(255,189,62) 50%, rgb(243,165,14) 100%);
  background-image: -ms-linear-gradient( 0deg, rgb(255,184,99) 0%, rgb(255,189,62) 50%, rgb(243,165,14) 100%);
  border-radius: 41rpx;text-align: center;line-height: 82rpx;margin: 0 auto;color: #fff;font-size: 30rpx;margin-top: 70rpx}
  .layer_box .layer .none{font-size: 30rpx;color: #F3A50F;display: block;text-align: center;height: 30rpx;line-height: 30rpx;
  margin-top: 32rpx}